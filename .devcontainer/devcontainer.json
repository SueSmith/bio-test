{
  "name": "Ubuntu",
  "image": "mcr.microsoft.com/devcontainers/base:noble",
  "features": {
    "ghcr.io/devcontainers/features/node:1": {}
  },
  "portsAttributes": {
    "7676": {
      "label": "Compute",
      "onAutoForward": "openPreview"
    }
  },
  "updateContentCommand": "npm install",
  "postAttachCommand": {
    "start": "npm run start",
    "build": "npm run build"
  },
  "waitFor": "updateContentCommand"
}
